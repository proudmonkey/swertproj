

@model List<Swertres.Web.Models.ViewModels.BetSummaryModel>
<div>
    <table class="table table-striped table-condensed table-hover">
        <thead>
            <tr>
                <th>Number</th>
                <th>Total Excess Amount Normal</th>
                <th>Total Excess Amount Rumble</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var i in Model)
            {
                <tr>
                    <td> @Html.DisplayFor(m => i.Number)</td>
                    <td> @Html.DisplayFor(m => i.TotalAmountNormalExcess)</td>
                    <td> @Html.DisplayFor(m => i.TotalAmountRumbleExcess)</td>
                </tr>
            }
        </tbody>
    </table>
</div>
